<?xml version="1.0" encoding="UTF-8"?>
<!-- Template for the service call dashboard -->
<templates id="service_call_dashboard_templates" xml:space="preserve">
	<t t-name="service_call_dashboard_odoo.ServiceProjectDashboard">
		<div class="oh_service_call_dashboards">
			<link rel="stylesheet" href="/service_call_dashboard_odoo/static/src/css/service_dashboard.css"/>
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.css"/>
			<button class="sidebar-toggle-btn" t-on-click="toggleMainSidebar">
				<i class="bi bi-chevron-double-right"/>
			</button>
			<!-- Main Sidebar -->
			<div class="main-sidebar">
				<div class="sidebar-header">
					<h3>
						<t t-if="state.currentView === 'companies'">Companies</t>
						<t t-elif="state.currentView === 'departments'">
							<i class="fa fa-building" t-on-click="showCompanies"/>
                            Departments

						</t>
						<t t-elif="state.currentView === 'subdepartments'">
							<i class="fa fa-building" t-on-click="showDepartments"/>
                            Sub Departments

						</t>
					</h3>
				</div>
				<div class="sidebar-content">
					<!-- Companies List with Nested Structure -->
					<div t-if="state.currentView === 'companies'" class="company-list">
						<t t-foreach="state.companies" t-as="company" t-key="company.id">
							<div class="company-item" t-att-class="{'active': state.selectedCompany?.id === company.id}"
                                 t-on-click="() => this.toggleCompany(company)">
								<i class="fa fa-building"/>
								<span t-esc="company.name"/>
								<i t-attf-class="fa fa-chevron-{{ state.selectedCompany?.id === company.id ? 'down' : 'right' }} ms-auto"/>
							</div>
							<!-- Departments (shown when company is selected) -->
							<div t-if="state.selectedCompany?.id === company.id" class="department-list nested-list">
								<t t-foreach="state.departments" t-as="department" t-key="department.id">
									<t t-if="department.name == 'Service Division'">
										<div class="department-item"
                                         t-att-class="{'active': state.selectedDepartment?.id === department.id}"
                                         t-on-click="() => this.toggleDepartment(department)">
											<i class="fa fa-sitemap"/>
											<span t-esc="department.name"/>
											<i t-attf-class="fa fa-chevron-{{ state.selectedDepartment?.id === department.id ? 'down' : 'right' }} ms-auto"/>
										</div>
									</t>
									<!-- Sub-departments (shown when department is selected) -->
									<div t-if="state.selectedDepartment?.id === department.id" class="subdepartment-list nested-list">
										<t t-foreach="state.subdepartments" t-as="subdepartment" t-key="subdepartment.id">
											<div class="subdepartment-item"
                                             t-att-class="{'active': state.selectedSubDepartment?.id === subdepartment.id}"
                                             t-on-click="() => this.selectSubDepartment(subdepartment)">
												<i class="fa fa-sitemap"/>
												<span t-esc="subdepartment.name"/>
												<i t-attf-class="fa fa-chevron-{{ state.selectedSubDepartment?.id === subdepartment.id ? 'down' : 'right' }} ms-auto"/>
											</div>
											<!-- Display States when a Subdepartment is selected -->
											<div t-if="state.selectedSubDepartment?.id === subdepartment.id"
                                                 class="state-list nested-list">
												<t t-foreach="state.states" t-as="stateItem" t-key="stateItem.id">
													<t t-if="stateItem.subdepartment_id === subdepartment.id">
														<div class="state-item"
                                                             t-att-class="{'active': state.selectedState?.id === stateItem.id}"
                                                             t-on-click="() => this.selectState(stateItem)">
															<i class="fa fa-map-marker"/>
															<span t-esc="stateItem.name"/>
															<i t-attf-class="fa fa-chevron-{{ state.selectedState?.id === stateItem.id ? 'down' : 'right' }} ms-auto"/>
														</div>
														<!-- Display Cities when this State is selected -->
														<div t-if="state.selectedState?.id === stateItem.id"
                                                             class="city-list nested-list">
															<t t-foreach="state.cities" t-as="cityItem" t-key="cityItem.id">
																<div class="city-item"
                                                                     t-att-class="{'active': state.selectedCities.includes(cityItem.name)}">
																	<span class="city-checkbox-wrapper">
																		<input type="checkbox"
                                                                               class="city-checkbox"
                                                                               id="city-{{ cityItem.id }}"
                                                                               t-att-checked="state.selectedCities.includes(cityItem.name)"
                                                                               t-on-change="() => this.toggleCitySelection(cityItem)" />
																	</span>
																	<span t-esc="cityItem.name"/>
																</div>
															</t>
															<div t-if="state.cities.length === 0" class="no-cities-message">
                                                                No cities found for this state
                                                            </div>
														</div>
													</t>
												</t>
											</div>
										</t>
									</div>
								</t>
							</div>
						</t>
					</div>

					<div class="sidebar-bottom">
						<div class="sidebar-footer">
							<button class="btn btn-primary sidebar-action-button"
                                t-on-click="applyDepartmentFilter"
                                style="background: #7C7BAD;"
                                t-att-disabled="state.isApplyingFilter">
								<t t-if="state.isApplyingFilter">
									<i class="fa fa-spinner fa-spin me-2"/>
                                Applying...

								</t>
								<t t-else="">
									<i class="fa fa-filter me-2"/>
                                Apply Filter

								</t>
							</button>
							<button class="btn btn-secondary sidebar-action-button"
                                    t-on-click="clearDepartmentFilter"
                                    style="background: #875A7B;"
                                    t-att-disabled="state.isClearingFilter">
								<t t-if="state.isClearingFilter">
									<i class="fa fa-spinner fa-spin me-2"/>
                                    Clearing...

								</t>
								<t t-else="">
									<i class="fa fa-times me-2"/>
                                    Clear Filter

								</t>
							</button>
						</div>
					</div>
					<!-- Departments List -->
					<div t-if="state.currentView === 'departments'" class="department-list">
						<t t-foreach="state.departments" t-as="department" t-key="department.id">
							<div class="company-item" t-on-click="() => this.selectDepartment(department)">
								<i class="fa fa-sitemap"/>
								<span t-esc="department.name"/>
								<i class="fa fa-chevron-right ms-auto"/>
							</div>
						</t>
					</div>
					<div t-if="state.currentView === 'states'" class="state-list">
						<t t-foreach="state.states" t-as="stateItem" t-key="stateItem.id">
							<div class="company-item" t-on-click="() => this.selectState(stateItem)">
								<i class="fa fa-flag"/>
								<span t-esc="stateItem.name"/>
								<i class="fa fa-chevron-right ms-auto"/>
							</div>
						</t>
					</div>
					<!-- Sub Departments List -->
					<div t-if="state.currentView === 'subdepartments'" class="subdepartment-list">
						<t t-foreach="state.subdepartments" t-as="subdepartment" t-key="subdepartment.id">
							<div class="company-item" t-on-click="() => this.selectSubDepartment(subdepartment)">
								<i class="fa fa-folder"/>
								<span t-esc="subdepartment.name"/>
							</div>
						</t>
					</div>
				</div>
			</div>
			<!-- Main Content with proper margin -->
			<div class="dashboard-content">
				<div class="container-fluid">
					<t t-call="service_call_dashboard_odoo.DashboardServiceCall"/>
					<t t-call="service_call_dashboard_odoo.DashboardCallChart"/>
				</div>
			</div>
		</div>
